<h1>Nutrition Facts Finder</h1>
<input type="text" id="foodInput" placeholder="Enter a food name" />
<button id="getNutritionBtn">Get Nutrition Info</button>

<div id="food-info">
    <h3>Nutrition Facts</h3>
    <p id="food-name"></p>
    <table>
        <thead>
            <tr>
                <th>Nutrient</th>
                <th>Amount</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>Calories</td>
                <td id="calories">-</td>
            </tr>
            <tr>
                <td>Fat</td>
                <td id="fat">-</td>
            </tr>
            <tr>
                <td>Carbohydrates</td>
                <td id="carbs">-</td>
            </tr>
            <tr>
                <td>Protein</td>
                <td id="protein">-</td>
            </tr>
        </tbody>
    </table>
</div>


<script src="~/js/health.js"></script>